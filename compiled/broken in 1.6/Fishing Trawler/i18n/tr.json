  // Murphy's dialogue
  "dialogue.player_title.male": "delikanlı",
  "dialogue.player_title.female": "genç hanım",
  "dialogue.introduction": "Ahoy {0}!$h#$b#Sen @ olmalısın. Willy bir arkadaşının uğrayacağını söylemişti..$n#$b#Yani, eski Şafak'ta bir gemici olmak istiyorsun öyle mi?$n#$b#Ödeyecek çok şeyim yok ama yakaladığımız balığın payı senin olsun!$h#$e",
  "dialogue.greeting": "Ahoy {0}!$h#$b#Deniz bugün bizim lehimize olsun. Yoba'nın kutsaması ile tek parça halinde geri geleceğiz.$n#$e",
  "dialogue.greeting_rainy": "Ahoy {0}!$h#$b#Deniz bugün çalkantılı, ama biz deniz kurtlarını etkilemez, değil mi?$n#$e",
  "dialogue.greeting_island": "Ahoy {0}!$h#$b#Okyanus esintisini hissederken güzel bir içeceğin tadını çıkarmak gibisi yok, değil mi {0}?$h#$e",
  "dialogue.options": "Balığa çıkalım mı, {0}?",
  "dialogue.confirm_first_trip": "Bu senin ilk gezin olacağı için, sana tayfanın görevlerini açıklamamı ister misin?",
  "dialogue.minigame_explanation_hull": "Gemici olarak asıl görevin, Eğreltiotu Adaları çevresinde dolaşırken Şafak'ı ayakta tutmak olacak..$n#$b#Bu yaşlı kızın daha iyi günleri de oldu, tıpkı benim gibi, ve o parçalanmaya meyilli.$s#$b#Gövdede deliklerin oluşması muhtemeldir, bu yüzden hızlıca yamadığıdan emin ol.$n#$b#Eğer çok fazla su alırsak geri dönmek zorunda kalırım ve avdaki payın çok az olur.$s#$b#",
  "dialogue.minigame_explanation_bailing": "Demir aldığımız zaman sana bir su boşaltma kovası vereceğim. Bir miktar su almak için teknede kullan ve su baskınını azaltmak için denize boşalt..$n#$b#",
  "dialogue.minigame_explanation_nets": "İkinci görevin balık ağlarının sağlam kaldığından emin olmak. Güvertede, kıç tarafına yakın vinçler tarafından tutuluyorlar..$n#$b#Sağlam ağ yoksa balık da yok.$s#$b#",
  "dialogue.minigame_explanation_engine": "Son olarak motor, kabinin içinde bulunuyor. Borular ara sıra buhar sızdırıyor ve yamalanmaları gerekiyor.$n#$b#Sızdıran bir boru, arızalı bir motor anlamına gelir. Arızalı bir motor, ağların o kadar çok balık tutmayacağı anlamına gelir. Hem ağlar hem de motor bozulursa, balıkları da kaybetmeye başlarız.$s#$b#",
  "dialogue.minigame_explanation_finish": "Eğer tek parça halinde geri dönersek bütün kazanç senindir. Ama eğer normalden erken dönmek zorunda kalırsak daha küçük bir pay alırsın.$n#$b#Bu iş için hazırsan bana söyle ve denize açılalım.$h#$e#",
  "dialogue.reward_explanation_flags": "Vay canına {0}! Bu benim düşündüğüm şey mi?$u#$b#%Murphy nazikçe bayrağı senden alıp dikkatle inceliyor.$n#$b#Bu, çok eskiden denizciler ve korsanlar tarafından dalgalandırılmış antik bir bayrak.$n#$b#Bu bayraklar ve onları kullanan denizcilere verdikleri efektler hakkında birsürü şey anlatılır.$n#$b#Bunu sana vereceğim {0}, ama eğer onu eski Şafak'ta dalgalandırmak istersen, ya da daha fazlasını bulursan bana bildir.$h#$e",
  "dialogue.after_trip_successful": "Aferin {0}! Senden tam bir deniz kurdu olur.$h#$b#Senin payın soldaki sandıkta!$n#$e",
  "dialogue.after_trip_failure": "Yoba'ya şükür ki tek parça döndük.$s#$b#Endişelenme {0}, her zaman tekrar deneyebilirsin.$n#$e",
  "dialogue.trip_finished": "Başka bir geziye çıkmak için sabırsızlanıyor musun?$n#$b#Üzgünüm {0}, önümüzdeki haftaya kadar beklemen gerekecek. Bu yaşlı kemiklerin dinlenmesi gerek.$h#$e",
  "dialogue.full_inventory": "Herkes iş ba-$n#$b#Ah! Üzgünüm {0}, çantanda kova için boş yer olması lazım.$u#$b#Güven bana, çok ihtiyacın olacak.$s#$e",
  "dialogue.deckhand_busy": "Eh?$u#$b#Görünüşe göre ahbaplarından biri çantasına falan bakıyor, {0}.$n#$bO da hazır olunca tekrar benimle konuş ve açılalım.",
  "dialogue.start_departure": "Herkes iş başına! Demir alıyoruz, yelkenler fora!",
  "dialogue.how_to_hoist_flag": "Antik bayraklardan birini göndere çekmek istiyorsun he?$n#$b#Birini bana ver ve yolculuğa çıkmadan önce güzelce dalgalandığından emin olayım.$h#$e",
  "dialogue.what_flag_is_hoisted_none": "Şu anda yaşlı Şafak'ta asılı bir bayrak yok.",
  "dialogue.what_flag_is_hoisted": "Şu anda yaşlı Şafak'ta  {0} asılı.",
  "dialogue.identify_flag": "Eh, {0}?$n#$b#Vay be! Başka bir bayrak daha mı buldun?$u#$b#%Murphy nazikçe bayrağı senden alıp dikkatle inceliyor.$n#$b#Bu şüphesiz antik bayraklardan bir diğeri. Deniz sana lütfediyor {0}!$n#$b#Bunu sana vereceğim {0}, ama eğer onu eski Şafak'ta dalgalandırmak istersen, ya da daha fazlasını bulursan bana bildir.$h#$e",
  "dialogue.given_flag_to_hoist": "Bu bayrak Şafak'a güzel bir katkı yapacak.!$h#$b#Denize açıldığımızda onu düzgün bir şekilde asacağım {0}.",
  "dialogue.given_flag_to_hoist_return_old": "Bu bayrak Şafak'a güzel bir katkı yapacak.!$h#$b#Denize açıldığımızda onu düzgün bir şekilde asacağım {0}.$h#$b#Oh! Unutmadan, bu da önceden kullandığımız bayrak.$n#$e",
  "dialogue.remove_current_flag": "Bayrağını geri mi istiyorsun {0}?$n#$b#Bu olmadan tekne biraz yalın kalacak, ama buyur!$h#$e",
  "dialogue.more_questions": "Sana nasıl yardımcı olabilirim, {0}?",
