{
    "$schema": "http://json-schema.org/draft-04/schema",
    "title": "JSON Schema for MushroomLogData content packs.",

    "type": "object",
    "additionalProperties": {
        "type": "array",
        "minItems": 1,
        "items": {
            "type": "object",
            "properties": {
                "Item1": {
                    "type": "string"
                },
                "Item2": {
                    "type": "number",
                    "minimum": 0,
                    "maximum": 1
                }
            },
            "required": [ "Item1", "Item2" ]
        }
    }
}
