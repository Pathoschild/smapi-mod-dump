<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <PropertyGroup>
        <Authors>furyx639</Authors>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <RepositoryUrl>https://github.com/ImJustMatt/StardewMods</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <TargetFramework>net452</TargetFramework>
        <LangVersion>latest</LangVersion>
        <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
        <GamePath>G:\Steam\steamapps\common\Stardew Valley 1.5.4</GamePath>
        <ModZipPath>$(SolutionDir)\releases</ModZipPath>
        <CodeAnalysisRuleSet>$(SolutionDir)\Custom.ruleset</CodeAnalysisRuleSet>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Pathoschild.Stardew.ModBuildConfig" Version="3.3.0" />
    </ItemGroup>
</Project>