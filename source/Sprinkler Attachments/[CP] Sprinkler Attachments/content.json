    "Format": "2.2.0",
    "Changes": [
        {
            "Action": "Load",
            "Target": "Mod/{{ModId}}/Objects",
            "FromFile": "assets/{{TargetWithoutPath}}.png",
            "When": {
                "mushymato.SprinklerAttachments/InvisibleAttachments": false
            }
        },
        {
            "Action": "Load",
            "Target": "Mod/{{ModId}}/Objects",
            "FromFile": "assets/{{TargetWithoutPath}}_Invisible.png",
            "When": {
                "mushymato.SprinklerAttachments/InvisibleAttachments": true
            }
        },
        {
            "Action": "Include",
            "FromFile": "data/Objects.json"
        },
        {
            "Action": "Include",
            "FromFile": "data/Shops.json"
        }
    ]
}