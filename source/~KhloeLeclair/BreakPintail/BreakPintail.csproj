<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>BreakPintail</AssemblyName>
    <Version>0.1.0</Version>
    <RootNamespace>Leclair.Stardew.BreakPintail</RootNamespace>

    <Name>Break Pintail</Name>
    <UniqueId>leclair.stardew.breakpintail</UniqueId>
    <Author>Khloe Leclair</Author>
    <Description>I'm here to make mods and break Pintail. And I'm all outta mods.</Description>
    <MinimumApiVersion>3.16</MinimumApiVersion>
    <UpdateKeys></UpdateKeys>
  </PropertyGroup>

  <Import Project="..\Common\Common.projitems" Label="Shared" />
  <Import Project="..\_build\common.targets" />

  <ItemGroup>
    <Reference Include="Nanoray.Pintail">
      <HintPath>$(GamePath)\smapi-internal\Pintail.dll</HintPath>
      <Private>false</Private>
    </Reference>
  </ItemGroup>

</Project>
