  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\QualityScrubber\QualityScrubber.csproj">
      <Private>false</Private>
    </ProjectReference>
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Automate">
      <HintPath>..\..\StardewValleyModding\references\Automate.dll</HintPath>
      <Private>false</Private>
    </Reference>
  </ItemGroup>

</Project>