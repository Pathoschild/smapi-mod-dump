  "Name": "Rainbow Fan",
  "$define": {
    "condition names": [
      "IsDrinking",
      "IsCasting",
      "IsFishing",
      "IsNibbling",
      "IsReeling",
      "IsUsingHeavyTool",
      "IsUsingMilkPail",
      "IsUsingShears",
      "IsUsingPan",
      "IsUsingSlingshot",
      "IsHarvesting",
      "IsCarrying",
      "IsSwimming",
      "IsInBathingSuit"
    ],
    "conditions array": {
      "$transform": "for-each",
      "var": "name",
      "in": { "$var": "condition names" },
      "yield": {
        "Name": { "$var": "name" },
        "Value": { "$var": "condition value" }
      }
    },
    "true conditions": {
      "$transform": "let",
      "bindings": { "condition value": true },
      "body": { "$var": "conditions array" }
    },
    "false conditions": {
      "$transform": "let",
      "bindings": { "condition value": false },
      "body": { "$var": "conditions array" }
    }
  },
