<Project Sdk="Microsoft.NET.Sdk">
	
	<PropertyGroup>
		<AssemblyName>StartWithGreenhouse</AssemblyName>
		<RootNamespace>StartWithGreenhouse</RootNamespace>
		<Version>1.0.1</Version>
		<TargetFramework>net5.0</TargetFramework>
		<LangVersion>latest</LangVersion>
		<ImplicitUsings>enable</ImplicitUsings>
		<EnableHarmony>false</EnableHarmony>
	</PropertyGroup>

	<Import Project="..\SullySDVcore\SullySDVcore.projitems" Label="Shared" />
	
	<ItemGroup>
		<PackageReference Include="Microsoft.Win32.Registry" Version="5.0.0" />
		<PackageReference Include="Pathoschild.Stardew.ModBuildConfig" Version="4.0.0" />
	</ItemGroup>

	<ItemGroup>
		<Using Include ="StardewValley.Object" Alias="SObject"/>
		<Using Include ="StardewValley"/>
		<Using Include ="StardewModdingAPI"/>
		<Using Include ="StardewModdingAPI.Events"/>
		<Using Include ="SullySDVcore"/>
	</ItemGroup>
</Project>