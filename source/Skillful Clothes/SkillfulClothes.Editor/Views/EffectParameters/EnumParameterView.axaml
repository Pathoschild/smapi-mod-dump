<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="SkillfulClothes.Editor.Views.EffectParameters.EnumParameterView">
  <Grid>
    <Grid.ColumnDefinitions>
      <ColumnDefinition Width="Auto" />
      <ColumnDefinition Width="*" />
    </Grid.ColumnDefinitions>

    <TextBlock Text="{Binding ParameterName}" VerticalAlignment="Center" />
    <ComboBox Grid.Column="1" SelectedItem="{Binding Value}" Items="{Binding AvailableValues}" Margin="10 0 0 0" />
  </Grid>
</UserControl>
