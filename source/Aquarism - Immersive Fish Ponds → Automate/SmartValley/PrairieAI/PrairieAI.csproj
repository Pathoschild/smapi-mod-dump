<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<AssemblyName>PrairieAI</AssemblyName>
	<RootNamespace>DaLion.Stardew.Prairie.Training</RootNamespace>
	<Version>1.0.0</Version>
	<TargetFramework>net5.0</TargetFramework>
	<EnableHarmony>true</EnableHarmony>
	<BundleExtraAssemblies>ThirdParty</BundleExtraAssemblies>
	<IgnoreModFilePaths>JetBrains.Annotations.dll</IgnoreModFilePaths>
  </PropertyGroup>

  <PropertyGroup>
	<LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
	<PackageReference Include="JetBrains.Annotations" Version="2021.3.0" />
	<PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
	<PackageReference Include="Pathoschild.Stardew.ModBuildConfig" Version="4.0.0" />
	<PackageReference Include="SharpNeatLib" Version="2.4.4" />
  </ItemGroup>

  <Import Project="..\Common\Common.projitems" Label="Shared" Condition="Exists('..\Common\Common.projitems')" />

</Project>