<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>BlueberryMushroomAutomation</AssemblyName>
    <RootNamespace>BlueberryMushroomAutomation</RootNamespace>
    <Version>2.2.0</Version>
    <EnableHarmony>true</EnableHarmony>
	<TargetFramework>net5.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Pathoschild.Stardew.ModBuildConfig" Version="4.1.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\BlueberryMushroomMachine\BlueberryMushroomMachine.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Automate">
      <HintPath>$(GamePath)\Mods\[CORE]\Automate\Automate.dll</HintPath>
      <Private>false</Private>
    </Reference>
  </ItemGroup>

  <ProjectExtensions><VisualStudio><UserProperties manifest_1json__JsonSchema="" /></VisualStudio></ProjectExtensions>

</Project>