	/*********
	** Configuration
	*********/
	"ConfigSchema": {
		"RepeatCableCarCutscene": {
			"AllowValues": "true, false",
			"Default": "true",
			"Description": "Enable Cable Car Cutscene."
		},

		"EnableOtherNPCsInCableCar": {
			"AllowValues": "true, false",
			"Default": "true",
			"Description": "Enable the chances of other NPCs to appear riding the Cable Car (Along with LASV, ES and SVE NPCs)."
		},

		"EnableRidgesideMusic": {
			"AllowValues": "true, false",
			"Default": "true",
			"Description": "Enables music for Ridgeside Village."
		},

		"RSVNPCSAttendFestivals": {
			"AllowValues": "true, false",
			"Default": "true",
			"Description": "Allows RSV NPCs to attend Pelican Town Festivals."
		},

		"ExpandedFestivalMaps": {
			"AllowValues": "true, false",
			"Default": "true",
			"Description": "Enables the expanded festival maps. If false will use the old placements that may conflict with other mods."
		},

		"EasyIntroduction": {
			"AllowValues": "true, false",
			"Default": "false",
			"Description": "RSV NPCs don't appear in Introduction quest."
		},

		"EnableBetterBusStop": {
			"AllowValues": "true, false",
			"Default": "true",
			"Description": "Uses Ridgeside Village's official patch for the Bus Stop by TheLimeyDragon and Rafseazz. Might be incompatible with other mods."
		},

		"PoleAtBackwoods": {
			"AllowValues": "true, false",
			"Default": "true",
			"Description": "Enable/Disable the cable car pole at the backwoods."
		},

		"SeasonalRSVMap": {
			"AllowValues": "true, false",
			"Default": "true",
			"Description": "Enable/Disable the seasonal RSV world map."
		},

		"EnableTouristNPCs": {
			"AllowValues": "true, false",
			"Default": "true",
			"Description": "Enable/Disable the decoration tourist NPCs in the hotel."
		},

		"EnableWTDRCompat": {
			"AllowValues": "true, false",
			"Default": "false",
			"Description": "EXPERIMENTAL! Establishes better compat with Walk to Desert Redux."
		}

/*		"EnableVanillaEdits": {
