<Project Sdk="Microsoft.NET.Sdk" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

    <PropertyGroup>   
        <PackageVersion>0.5.0</PackageVersion>
        <Authors>Slothsoft</Authors>
        <RepositoryType>git</RepositoryType>
        <RepositoryUrl>https://github.com/slothsoft/stardew-challenger</RepositoryUrl>
        <PackageProjectUrl>https://github.com/slothsoft/stardew-challenger/blob/main/README.md</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageLicenseUrl>https://github.com/slothsoft/stardew-challenger/blob/master/LICENSE</PackageLicenseUrl>

        <TargetFramework>net5.0</TargetFramework>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Pathoschild.Stardew.ModBuildConfig" Version="4.0.1" />
    </ItemGroup>

    <ItemGroup>
        <Using Include="StardewValley" />
        <Using Include="StardewModdingAPI" />
        <Using Alias="Object" Include="System.Object" />
        <Using Alias="SObject" Include="StardewValley.Object" />
    </ItemGroup>
</Project>