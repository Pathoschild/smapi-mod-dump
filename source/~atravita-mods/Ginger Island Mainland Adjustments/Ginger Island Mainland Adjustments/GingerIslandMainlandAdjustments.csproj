<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\csproj_common" />

  <PropertyGroup>
    <AssemblyName>GingerIslandMainlandAdjustments</AssemblyName>
    <RootNamespace>GingerIslandMainlandAdjustments</RootNamespace>
    <Name>Ginger Island Mainland Adjustments</Name>
    <Description>Dialogue keys for NPCs headed to or away from Ginger Island, post-Ginger Island Schedules, and so on.</Description>
    <EnableHarmony>true</EnableHarmony>
    <ImplicitUsings>enable</ImplicitUsings>
    <Version>1.1.8</Version>
    <BaseManifest>..\..\cp_dependencies.json</BaseManifest>
    <UpdateKeys> Nexus:10004@CodePack, ModDrop:1098263@CodePack</UpdateKeys>
  </PropertyGroup>

  <PropertyGroup>
    <TranslationClassBuilder_AddGetByKey>true</TranslationClassBuilder_AddGetByKey>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" Version="2022.3.1" />
    <PackageReference Include="Microsoft.Win32.Registry" Version="5.0.0" /> <!-- Makes warnings go away, not sure what it does...-->
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\AtraCore\AtraCore.csproj" private="false" />
    <ProjectReference Include="..\..\Pintail\Pintail\Pintail.csproj" private="false" />
  </ItemGroup>

  <ItemGroup>
    <Using Include="StardewValley" />
    <Using Include="StardewModdingAPI" />
    <Using Include="JetBrains.Annotations" />
    <Using Include="System.Diagnostics.CodeAnalysis" />
    <Using Alias="NotNullAttribute" Include="System.Diagnostics.CodeAnalysis.NotNullAttribute" />
    <Using Alias="Object" Include="System.Object" />
    <Using Alias="SObject" Include="StardewValley.Object" />
  </ItemGroup>
</Project>